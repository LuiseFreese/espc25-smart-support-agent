{
  "name": "demo-06-agentic-retrieval",
  "version": "1.0.0",
  "description": "Agentic retrieval demo using query decomposition and parallel search",
  "type": "module",
  "main": "dist/agenticRetrieval.js",
  "scripts": {
    "dev": "ts-node src/agenticRetrieval.ts",
    "build": "tsc",
    "start": "node dist/agenticRetrieval.js",
    "ui": "ts-node src/server.ts",
    "ui:build": "tsc && node dist/server.js",
    "ui:unified": "tsc && node dist/server-unified.js",
    "ui:react": "concurrently \"npm run ui:react:dev\" \"npm run ui:react:server\"",
    "ui:react:dev": "vite",
    "ui:react:build": "tsc -b && vite build",
    "ui:react:server": "tsc && node dist/server-unified.js"
  },
  "keywords": [
    "azure",
    "openai",
    "search",
    "rag",
    "agentic"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/search-documents": "^12.0.0",
    "@fluentui/react": "^8.125.1",
    "@fluentui/react-components": "^9.72.7",
    "@fluentui/react-icons": "^2.0.315",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.3"
  }
}